{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":disableDependencyDashboard"
  ],
  "packageRules": [
      {
          "matchDatasources": ["docker"],
          "matchPackageNames": ["python"],
          "matchCurrentVersion": "3.9-bullseye",
          "enabled": "false"
      }
  ],
  "poetry": {
    "fileMatch": [
      "(^|/)pyproject\\.toml$"
    ],
    "rangeStrategy": "in-range-only"
  }
}
